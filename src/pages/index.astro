---
import LayoutIndex from "@layouts/Index.astro";
import Header from "../components/Header.astro";
import { SITE_TITLE, SITE_DESCRIPTION } from "../consts";
import Hero from "../components/Hero.astro";
import Avisos from "../components/Avisos.astro";
import Boletines from "../components/Boletines.astro";
import Noticias from "../components/Noticias.astro";
import QueHacemos from "../components/QueHacemos.astro";
---

<LayoutIndex
	title={SITE_TITLE}
	description={SITE_DESCRIPTION}
>
	<Hero />
	<Header logoUrl="/pclogo.svg" />
	<Avisos />
	<Noticias />
	<Boletines />
	<QueHacemos />
</LayoutIndex>
